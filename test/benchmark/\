public class fib {
  long fibbonacci(long n) {   
  long  a = 0;
  long b = 1;
  long temp = 0;

  for (long i = 0; i < n; i++) {
    temp = a+b; 
    a=b;
    b=temp;
  }
  return a;
  }
  
  public static void main( String[] args ) {

    long startTime = System.nanotime();
    long answer = fibbonacci(50);

    System.out.printf("Found answer: %ld", answer);
  }
}
